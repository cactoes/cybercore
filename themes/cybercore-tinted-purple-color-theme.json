{
    "name": "cybercore purple",
    "tokenColors": [
        {
            "name": "Global settings",
            "settings": {
                "foreground": "#A5A1B6"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#958bc0"
            }
        },
        {
            "name": "Boolean",
            "scope": "constant.language.false, constant.language.true",
            "settings": {
                "foreground": "#AEAABF"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#747085"
            }
        },
        {
            "name": "Identifier",
            "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
            "settings": {
                "foreground": "#9692A7"
            }
        },
        {
            "name": "Parameter",
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#A6A2B7"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
            "settings": {
                "foreground": "#9D99AF"
            }
        },
        {
            "name": "Function call",
            "scope": "entity.name.function, support.function",
            "settings": {
                "foreground": "#837F94"
            }
        },
        {
            "name": "Storage",
            "scope": "storage.type, storage.modifier",
            "settings": {
                "foreground": "#A7A3B8"
            }
        },
        {
            "name": "Modules",
            "scope": "support.module, support.node",
            "settings": {
                "foreground": "#ABA7BC",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Type",
            "scope": "support.type",
            "settings": {
                "foreground": "#868297"
            }
        },
        {
            "name": "Type",
            "scope": "entity.name.type, entity.other.inherited-class, storage.type.java, entity.name.type.class.java, support.type.primitive",
            "settings": {
                "foreground": "#958bc0"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#A8A4B9"
            }
        },
        {
            "name": "Class",
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#837F94"
            }
        },
        {
            "name": "Class variable",
            "scope": "variable.object.property, meta.field.declaration entity.name.function",
            "settings": {
                "foreground": "#868297"
            }
        },
        {
            "name": "Class method",
            "scope": "meta.definition.method entity.name.function",
            "settings": {
                "foreground": "#747085"
            }
        },
        {
            "name": "Function definition",
            "scope": "meta.function entity.name.function",
            "settings": {
                "foreground": "#747085"
            }
        },
        {
            "name": "Template expression",
            "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
            "settings": {
                "foreground": "#9692A7"
            }
        },
        {
            "name": "Namespace",
            "scope": "entity.name.scope-resolution, entity.name.namespace",
            "settings": {
                "foreground": "#8F8BA0"
            }
        },
        {
            "name": "Reset embedded/template expression colors",
            "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
            "settings": {
                "foreground": "#ACA8BE"
            }
        },
        {
            "name": "JSON key",
            "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
            "settings": {
                "foreground": "#868297"
            }
        },
        {
            "name": "JSON constant",
            "scope": "constant.language.json",
            "settings": {
                "foreground": "#918DA3"
            }
        },
        {
            "name": "CSS class",
            "scope": "entity.other.attribute-name.class",
            "settings": {
                "foreground": "#837F94"
            }
        },
        {
            "name": "CSS ID",
            "scope": "entity.other.attribute-name.id",
            "settings": {
                "foreground": "#837F94"
            }
        },
        {
            "name": "CSS tag",
            "scope": "source.css entity.name.tag",
            "settings": {
                "foreground": "#837F94"
            }
        },
        {
            "name": "HTML tag outer",
            "scope": "meta.tag, punctuation.definition.tag",
            "settings": {
                "foreground": "#868297"
            }
        },
        {
            "name": "HTML tag inner",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#ABA7BC"
            }
        },
        {
            "name": "HTML tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#6D697E"
            }
        },
        {
            "name": "Markdown heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#89859A"
            }
        },
        {
            "name": "Markdown link text",
            "scope": "text.html.markdown meta.link.inline, meta.link.reference",
            "settings": {
                "foreground": "#89859A"
            }
        },
        {
            "name": "Markdown list item",
            "scope": "text.html.markdown beginning.punctuation.definition.list",
            "settings": {
                "foreground": "#89859A"
            }
        },
        {
            "name": "Markdown italic",
            "scope": "markup.italic",
            "settings": {
                "foreground": "#ABA7BC",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown bold",
            "scope": "markup.bold",
            "settings": {
                "foreground": "#ABA7BC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown bold italic",
            "scope": "markup.bold markup.italic, markup.italic markup.bold",
            "settings": {
                "foreground": "#ABA7BC",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Markdown code block",
            "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
            "settings": {
                "foreground": "#89859A"
            }
        },
        {
            "name": "Markdown inline code",
            "scope": "markup.inline.raw.string.markdown",
            "settings": {
                "foreground": "#89859A"
            }
        },
        {
            "name": "INI property name",
            "scope": "keyword.other.definition.ini",
            "settings": {
                "foreground": "#89859A"
            }
        },
        {
            "name": "INI section title",
            "scope": "entity.name.section.group-title.ini",
            "settings": {
                "foreground": "#89859A"
            }
        },
        {
            "name": "C++ preprocessor",
            "scope": "source.cpp meta.preprocessor, keyword.control.directive",
            "settings": {
                "foreground": "#868297"
            }
        },
        {
            "name": "C++ preprocessor",
            "scope": "source.cpp meta.preprocessor.pragma.cpp",
            "settings": {
                "foreground": "#6D697E"
            }
        },
        {
            "name": "c++ user-defined literal",
            "scope": "keyword.other.unit.user-defined.cpp",
            "settings": {
                "foreground": "#868297"
            }
        },
        {
            "name": "C++ preprocessor macros",
            "scope": "source.cpp meta.preprocessor, entity.name.function.preprocessor.cpp",
            "settings": {
                "foreground": "#b58bc0"
            }
        },
        {
            "name": "C++ variable property",
            "scope": "variable.other.property, variable.other.enummember",
            "settings": {
                "foreground": "#A29EB3"
            }
        },
        {
            "name": "Java import",
            "scope": "storage.modifier.import.java",
            "settings": {
                "foreground": "#958bc0"
            }
        },
        {
            "name": "TOML entry",
            "scope": "support.type.property-name.toml",
            "settings": {
                "foreground": "#6D697E"
            }
        },
        {
            "name": "TOML table",
            "scope": "support.type.property-name.table.toml",
            "settings": {
                "foreground": "#A39FB5"
            }
        },
        {
            "name": "CMAKE variable source",
            "scope": "variable.source.cmake",
            "settings": {
                "foreground": "#8B879D"
            }
        },
        {
            "name": "CMAKE storage source",
            "scope": ", storage.source.cmake",
            "settings": {
                "foreground": "#8B879D"
            }
        },
        {
            "name": "Global settings",
            "settings": {
                "foreground": "#ACA8BE"
            }
        }
    ],
